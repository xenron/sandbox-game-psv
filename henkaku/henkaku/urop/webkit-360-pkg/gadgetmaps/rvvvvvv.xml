<?xml version="1.0"?>

<gadgetmap>
	<regex>.*(\(rv{0,6}\))</regex>
	<stack>
		<data>GADGET_ADDRESS(pop_r1_r2_r3_r4_pc)</data>
		<data>ARG1</data> 		         					<!-- R1 -->
		<data>ARG2</data> 		         					<!-- R2 -->
		<data>ARG3</data> 		         					<!-- R3 -->
		<data>FUNC_ADDRESS</data> 		         			<!-- R4 -->
		<data>GADGET_ADDRESS(blx_r4-pop_r4_r5_r6_pc)</data>	<!-- PC -->
		<data>ARG4</data>                       			<!-- R4 -->
		<data>ARG5</data>                       			<!-- R5 -->
		<data>ARG6</data>                       			<!-- R6 -->
		<data>RETURN_ADDRESS</data>		 					<!-- PC return -->
	</stack>

	<testcase>
!! execute function !!
----------------------
0 = RETVAL
1 = ARG1
2 = ARG2
3 = ARG3
4 = ARG4
5 = ARG5
6 = ARG6
7 = .*
8 = .*
9 = .*
----------------------
END; r0 = RETVAL
	</testcase>
</gadgetmap>

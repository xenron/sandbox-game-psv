<?xml version="1.0"?>

<gadgetmap>
	<regex>.*(\(v{0,3}\))</regex>
	<stack>
		<data>GADGET_ADDRESS(pop_r0_r1_r2_r3_r4_r5_pc)</data>
		<data>ARG0</data> 		         					<!-- R0 -->
		<data>ARG1</data> 		         					<!-- R1 -->
		<data>ARG2</data> 		         					<!-- R2 -->
		<data>FUNC_ADDRESS</data> 		         			<!-- R3 -->
		<data>0</data>                                      <!-- R4 -->
		<data>0</data>                                      <!-- R5 -->
		<data>GADGET_ADDRESS(blx_r3-pop_r4_pc)</data>       <!-- PC -->
		<data>0</data>                       				<!-- R4 -->
		<data>RETURN_ADDRESS</data>		 					<!-- PC return -->
	</stack>

	<testcase>
!! execute function !!
----------------------
0 = ARG0
1 = ARG1
2 = ARG2
3 = .*
4 = .*
5 = .*
6 = .*
7 = .*
8 = .*
9 = .*
----------------------
END; r0 = RETVAL
	</testcase>
</gadgetmap>
